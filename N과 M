# 15651
# N과 M
# 순열을 구하는 문제
N, M = map(int, input().split())
value = []
def dfs(level, init):
    if level == M:
        value.append(init)
        return
    for idx in range(1, N+1):
        temp = init[:]
        temp.append(idx)
        dfs(level+1, temp)

for i in range(1, N+1):
    dfs(1, [i])

for l in range(len(value)):
    print(*value[l])
